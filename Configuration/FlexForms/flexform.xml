<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
		<langChildren>1</langChildren>
	</meta>
	<sheets>
		<sDEF>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:ad_google_maps/Resources/Private/Language/locallang_flexform.xml:sheetGoogleMap</sheetTitle>
					<cshFile>EXT:ad_google_maps/Resources/Private/Language/locallang_flexform_csh.xml</cshFile>
				</TCEforms>
				<type>array</type>
				<el>
					<settings.flexform.mapUid>
						<TCEforms>
							<label>LLL:EXT:ad_google_maps/Resources/Private/Language/locallang_flexform.xml:settings.flexform.mapUid</label>
							<exclude>1</exclude>
							<config>
								<type>select</type>
								<foreign_table>tx_adgooglemaps_domain_model_map</foreign_table>
								<foreign_table_where>AND (tx_adgooglemaps_domain_model_map.sys_language_uid = 0 OR tx_adgooglemaps_domain_model_map.l18n_parent = 0)</foreign_table_where>
								<suppress_icons>1</suppress_icons>
								<minitems>1</minitems>
								<maxitems>1</maxitems>
								<size>1</size>
							</config>
						</TCEforms>
					</settings.flexform.mapUid>
					<settings.flexform.width>
						<TCEforms>
							<label>LLL:EXT:ad_google_maps/Resources/Private/Language/locallang_flexform.xml:settings.flexform.width</label>
							<exclude>1</exclude>
							<config>
								<type>input</type>
								<size>2</size>
								<eval>int</eval>
							</config>
						</TCEforms>
					</settings.flexform.width>
					<settings.flexform.height>
						<TCEforms>
							<label>LLL:EXT:ad_google_maps/Resources/Private/Language/locallang_flexform.xml:settings.flexform.height</label>
							<exclude>1</exclude>
							<config>
								<type>input</type>
								<size>2</size>
								<eval>int</eval>
							</config>
						</TCEforms>
					</settings.flexform.height>
					<settings.flexform.templates>
						<TCEforms>
							<label>LLL:EXT:ad_google_maps/Resources/Private/Language/locallang_flexform.xml:settings.flexform.templates</label>
							<exclude>1</exclude>
							<config>
								<type>select</type>
								<itemsProcFunc>tx_AdGoogleMaps_Service_FlexFormTemplateSelection->getItems</itemsProcFunc>
								<size>3</size>
								<autoSizeMax>10</autoSizeMax>
								<minitems>1</minitems>
								<maxitems>99</maxitems>
							</config>
						</TCEforms>
					</settings.flexform.templates>
					<settings.flexform.categories>
						<TCEforms>
							<label>LLL:EXT:ad_google_maps/Resources/Private/Language/locallang_flexform.xml:settings.flexform.categories</label>
							<exclude>1</exclude>
							<displayCond>EXT:cps_tcatree:LOADED:false</displayCond>
							<config>
								<type>select</type>
								<size>3</size>
								<autoSizeMax>15</autoSizeMax>
								<minitems>0</minitems>
								<maxitems>99</maxitems>
								<foreign_table>tx_adgooglemaps_domain_model_category</foreign_table>
								<foreign_table_where>AND (tx_adgooglemaps_domain_model_category.sys_language_uid = 0 OR tx_adgooglemaps_domain_model_category.l18n_parent = 0) ORDER BY tx_adgooglemaps_domain_model_category.sorting</foreign_table_where>
							</config>
						</TCEforms>
					</settings.flexform.categories>
					<settings.flexform.categoriesCpsTcaTree>
						<TCEforms>
							<label>LLL:EXT:ad_google_maps/Resources/Private/Language/locallang_flexform.xml:settings.flexform.categories</label>
							<exclude>1</exclude>
							<displayCond>EXT:cps_tcatree:LOADED:true</displayCond>
							<config>
								<type>select</type>
								<form_type>user</form_type>
								<userFunc>tx_cpstcatree->getTree</userFunc>
								<size>3</size>
								<autoSizeMax>15</autoSizeMax>
								<minitems>0</minitems>
								<maxitems>99</maxitems>
								<foreign_table>tx_adgooglemaps_domain_model_category</foreign_table>
								<foreign_table_where>AND (tx_adgooglemaps_domain_model_category.sys_language_uid = 0 OR tx_adgooglemaps_domain_model_category.l18n_parent = 0) ORDER BY tx_adgooglemaps_domain_model_category.sorting</foreign_table_where>
								<treeView>1</treeView>
								<expandable>1</expandable>
								<expandFirst>0</expandFirst>
								<expandAll>0</expandAll>
							</config>
						</TCEforms>
					</settings.flexform.categoriesCpsTcaTree>
				</el>
			</ROOT>
		</sDEF>
	</sheets>
</T3DataStructure>
